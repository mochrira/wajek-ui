{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wajek-ui": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "allowedCommonJsDependencies": [],
            "outputPath": {
              "browser": "",
              "base": "www"
            },
            "deleteOutputPath": false,
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/.htaccess",
              "src/favicon.ico",
              "src/assets",
              "src/docs"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
            ],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "src/main.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "."
              ]
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "wajek-ui:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "wajek-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "wajek-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              
              "src/styles.scss"
            ],
            "scripts": [
              
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        }
      }
    },
    "wajek-ui-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "wajek-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wajek-ui:serve:production"
            }
          }
        }
      }
    },
    "wui": {
      "root": "projects/wui",
      "sourceRoot": "projects/wui/src",
      "projectType": "library",
      "prefix": "wui",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {  
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/wui/tsconfig.lib.json",
            "project": "projects/wui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/wui/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wui/src/test.ts",
            "tsConfig": "projects/wui/tsconfig.spec.json",
            "karmaConfig": "projects/wui/karma.conf.js"
          }
        }
      }
    },
    "focus": {
      "projectType": "library",
      "root": "projects/focus",
      "sourceRoot": "projects/focus/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/focus/tsconfig.lib.json",
            "project": "projects/focus/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/focus/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/focus/src/test.ts",
            "tsConfig": "projects/focus/tsconfig.spec.json",
            "karmaConfig": "projects/focus/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}